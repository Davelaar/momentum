
[Unit]
Description=Momentum Public WS (v2 subscribe)
After=network.target

[Service]
Type=simple
User=snapdiscounts
Group=psacln
WorkingDirectory=/var/www/vhosts/snapdiscounts.nl/momentum
Environment=APP=/var/www/vhosts/snapdiscounts.nl/momentum
ExecStart=/var/www/vhosts/snapdiscounts.nl/momentum/.venv/bin/python -m momentum.scripts.ws_public_subscribe --app /var/www/vhosts/snapdiscounts.nl/momentum
Restart=always
RestartSec=2

[Install]
WantedBy=multi-user.target
